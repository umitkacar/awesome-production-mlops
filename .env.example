# MLOps Environment Variables Example
# Copy this file to .env and fill in your values

# LLM API Keys
OPENAI_API_KEY=your-openai-key-here
ANTHROPIC_API_KEY=your-anthropic-key-here
COHERE_API_KEY=your-cohere-key-here

# MLflow Configuration
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_S3_ENDPOINT_URL=http://localhost:9000
AWS_ACCESS_KEY_ID=minioadmin
AWS_SECRET_ACCESS_KEY=minioadmin

# Vector Database
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=

# Weights & Biases
WANDB_API_KEY=your-wandb-key-here
WANDB_PROJECT=mlops-project

# Neptune.ai
NEPTUNE_API_TOKEN=your-neptune-token-here
NEPTUNE_PROJECT=workspace/project

# Database
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=mlops
POSTGRES_PASSWORD=mlops123
POSTGRES_DB=mlops

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Prefect
PREFECT_API_URL=http://localhost:4200/api
PREFECT_API_KEY=

# Model Serving
MODEL_SERVING_PORT=8000
MODEL_SERVING_HOST=0.0.0.0

# Monitoring
EVIDENTLY_SERVICE_URL=http://localhost:8001

# Security
JWT_SECRET_KEY=your-secret-key-here
JWT_ALGORITHM=HS256

# Feature Store (Feast)
FEAST_REGISTRY_PATH=./feature_repo/data/registry.db
FEAST_ONLINE_STORE_TYPE=redis

# Environment
ENVIRONMENT=development  # development, staging, production
LOG_LEVEL=INFO
DEBUG=True
